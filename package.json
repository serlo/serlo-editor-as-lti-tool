{
  "name": "serlo-editor-lti-tool",
  "version": "0.0.1",
  "private": true,
  "license": "Apache-2.0",
  "author": "Serlo Education e.V.",
  "type": "module",
  "scripts": {
    "build": "tsc && vite build",
    "dev": "docker compose up --watch --build",
    "format": "prettier . --write",
    "lint": "tsc --noEmit",
    "mysql": "docker compose exec mysql serlo-mysql",
    "mysql:rollback": "docker compose exec mysql sh -c \"pv /docker-entrypoint-initdb.d/001-init.sql | serlo-mysql\"",
    "push-image": "yarn tsx push-image.ts",
    "push-image:dev": "yarn push-image dev",
    "push-image:latest": "yarn push-image latest",
    "start:dev": "tsx --watch src/backend/index.ts"
  },
  "dependencies": {
    "@serlo/editor": "0.7.2",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "fp-ts": "2.16.6",
    "io-ts": "^2.2.21",
    "jsonwebtoken": "9.0.2",
    "jwt-decode": "4.0.0",
    "ltijs": "^5.9.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tsx": "4.15.6",
    "typescript": "^5.4.5"
  },
  "devDependencies": {
    "@types/jsonwebtoken": "9.0.6",
    "@types/ltijs": "4.0.11",
    "@types/node": "^20.12.12",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "prettier": "3.3.2",
    "prettier-plugin-packagejson": "^2.5.0",
    "vite": "^5.2.0"
  },
  "packageManager": "yarn@4.2.2"
}
